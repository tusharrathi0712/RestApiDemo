        <script type="text/javascript" src="/../js/all.js"></script>
    </head>
    <body>
        <form method="post" action="/users/add">
                <table cellpadding="11">
                <tr>
                    <td class="label">FirstName</td><td>: <input type="text" name="firstname" required></td>
