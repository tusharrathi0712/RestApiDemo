                 <tr>
                    <td class="label">LastName</td><td>: <input type="text" name="lastname" required></td>
                </tr>
                <tr>
                    <td class="label">Email</td><td>: <input type="text" name="email" required></td>
                 </tr>
                  <tr>
                    <td class="label">Id</td><td>: <input type="text" name="id" required></td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
        </body>
</html>

